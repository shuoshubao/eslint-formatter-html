{
  "name": "eslint-formatter-html",
  "version": "2.1.4",
  "description": "eslint-formatter-html",
  "files": [
    "dist",
    "index.js",
    "index.html"
  ],
  "repository": {
    "url": "https://github.com/shuoshubao/eslint-formatter-html.git",
    "type": "git"
  },
  "author": "shuoshubao <759979885@qq.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-formatter",
    "eslint-formatter-html"
  ],
  "scripts": {
    "start:css": "sass lib/index.scss dist/index.css -w --no-source-map",
    "build:css": "sass lib/index.scss dist/index.css --no-source-map --style=compressed",
    "start:js": "esbuild lib/index.js --outfile=dist/index.js --loader:.js=jsx --bundle --watch",
    "build:js": "esbuild lib/index.js --outfile=dist/index.js --loader:.js=jsx --bundle --minify",
    "start": "npm run start:css & npm run start:js",
    "build": "npm run build:css && npm run build:js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "strip-ansi": "^5.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.15.16",
    "sass": "^1.56.1"
  }
}
